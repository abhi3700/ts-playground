{
  "name": "ts-playground",
  "version": "1.0.0",
  "description": "Playground for TypeScript",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist && rm -rf .nyc_output",
    "clean:all": "rm -rf dist && rm -rf node_modules && rm -rf .nyc_output",
    "start": "tsc && node dist/index.js",
    "test": "mocha -r ts-node/register tests/**/*.test.ts",
    "coverage": "nyc -r text -e .ts -x \"tests/*.test.ts\" npm run test",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "doc": "typedoc",
    "eg:hello": "tsc && node dist/examples/hello.js",
    "eg:async-1": "tsc && node dist/tuts/async/1.js",
    "eg:lite-server": "lite-server"
  },
  "keywords": [],
  "author": "abhi3700",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/jest": "^29.5.10",
    "@types/mocha": "^10.0.6",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "chai": "^4.3.10",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.4",
    "typescript": "^5.3.2"
  },
  "dependencies": {}
}
